	<input type="checkbox" id="has_4g" value="1"/>
	<label for="has_4g"><%= image_tag("4g.png",:size=>"40x40") %></label>	
</li>
</ul>
<ul id="loader_ajax">
	<li><img src="http://localhost:3000/assets/hr_ajax.gif" /></li>
</ul>

<ul id="operator">
<li name="Airtel">
<input type="checkbox" name="operator" value="1" checked="checked" class="operator_check"/>
</label><%= image_tag("airtel.png")%>Airtel
</li>

