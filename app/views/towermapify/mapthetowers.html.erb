<div id="map_container">
<div id="map_overlay_autocomplete">
<ul>
<li id="search_coverage">
	<label>
		<input type="text"  id="location" x-webkit-speech placeholder="Search coverage in.." size="35"/>
	</label>
</li>
<li>
	<label>
		<input type="button" value="resetmap" onclick="Reset_map_to_Original();"/>
	</label>
</li>

</ul>
</div>

<div id="map_canvas">
</div>
<form action="#" id="form_id">

<ul id="map_operator_list">
<div id="operator_div">
