<% @query.each do |l| %>
 <Placemark>
      <name><%= l.city %></name>
      
<description>

<![CDATA[
<% if l.has_3g then %>
<%= image_tag("3g.jpg") %>
<%end%>
<% if l.has_4g then %>
<%= image_tag("4g.png") %>
<%end%>
<%= l.lon %>,<%= l.lat %>
]]>
</description>
  <styleUrl>#<%= getstyle(l.provider_id) %>icon</styleUrl>
      <Point>
        <coordinates><%= l.lon %>,<%= l.lat %></coordinates>
      </Point>
    </Placemark>
<% end %>




